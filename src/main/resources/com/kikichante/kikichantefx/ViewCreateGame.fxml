<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="bgImage" stylesheets="@style.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.kikichante.controller.ControllerCreateGame">

    <children>
          <Button alignment="CENTER" layoutX="86.0" layoutY="235.0" mnemonicParsing="false" onAction="#onClickCreateGame" prefHeight="66.0" prefWidth="154.0" text="Create Game">
         <font>
            <Font name="Cooper Black" size="17.0" />
         </font>
      </Button>
          <TextField fx:id="textfieldGameName" alignment="CENTER" layoutX="330.0" layoutY="75.0" prefHeight="78.0" prefWidth="174.0" promptText="Nom de la partie">
         <font>
            <Font name="Cooper Black" size="12.0" />
         </font>
      </TextField>

        <VBox />
        <Button layoutX="26.0" layoutY="30.0" mnemonicParsing="false" onAction="#onClickReturn" text="Retour">
         <font>
            <Font name="Cooper Black" size="14.0" />
         </font></Button>
    </children>
</AnchorPane>
